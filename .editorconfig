[*.cs]
csharp_new_line_before_open_brace = none

# Remove spaces before and after operators
csharp_space_around_binary_operators = ignore
csharp_space_between_method_declaration_parameter_parentheses = false

# Don't add space before open parentheses in method declarations or calls
csharp_space_before_open_square_brackets = false
csharp_space_before_open_parentheses = false